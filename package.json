  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "acorn": "^6.0.4",
    "aliasify": "^2.1.0",
    "autoprefixer": "^9.3.1",
    "babelify": "^10.0.0",
    "beeper": "^1.1.1",
    "brfs": "^2.0.1",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "chalk": "^2.4.1",
    "del": "^3.0.0",
    "favicons": "^5.2.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-cache": "^1.0.2",
    "gulp-concat": "^2.6.1",
