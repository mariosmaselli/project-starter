    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.0.9",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svgmin": "^2.1.0",
    "gulp-uglify": "^3.0.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pretty-hrtime": "^1.0.3",
    "through2": "^3.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development gulp --env dev",
    "prod": "NODE_ENV=production gulp --env prod"
  },
  "directories": {
    "src": "./src",
    "dist": "./dist",
    "test": "./test"
  },
  "config": {
    "verbose": false,
    "port": 3000,
    "browsers": [
      "last 2 version"
    ],
    "prodURL": "http://domain.tld",
    "shareImageURL": "http://domain.tld/static/share.png",
    "twitterImage": "https://domain.tld/static/twitter.png",
    "twitterHandle": "@domain",
    "themeColor": "#eeeeee",
    "analyticsUA": "UA-XXXXX-X",
    "developerURL": ""
  },
  "extensions": {
    "scripts": "js",
    "styles": "css"
  },
  "browserify": {
    "transform": [
      "brfs",
      "babelify"
    ]
  }
}
