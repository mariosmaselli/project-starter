{
  "name": "gulp-project-starter",
  "title": "Sarter Frontend Boilerplate",
  "version": "2.0.0",
  "description": "An ES6 boilerplate using Gulp 4 as build system.",
  "author": "mariosmaselli",
  "homepage": "https://github.com/mariosmaselli/project-starter",
