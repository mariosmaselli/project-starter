  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "browser-sync": "^2.26.3",
    "chalk": "^2.4.1",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.0.9",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.8",
    "minimist": "^1.2.0"
  },
  "directories": {
    "src": "./src",
    "dist": "./dist",
    "test": "./test"
